<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
 assembly="registros_online"
 namespace="registros_online.Models"
 auto-import="true">
  <class name="Register" table="dbo.movimientos">
    <id name="id" column="Id" type="int">
      <column name="id" />
      <generator class="identity" />
    </id>

    <property name="tipo" access="property"
     column ="tipo" type="integer"></property>

    <property name="fecha" access="property"
     column ="fecha" type="DateTime"></property>

    <property name="descripcion" access="property"
     column ="descripcion" type="String"></property>


    <property name="movimiento" access="property"
     column ="movimiento" type="String"></property>
    
    <property name="precio" access ="property"
     column ="preciounitario" type="Double"></property>
    
    <property name="id_usuario" access ="property"
      column ="id_user" type="integer"></property>

    
  </class>

</hibernate-mapping>